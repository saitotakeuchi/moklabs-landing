#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../next@14.2.33_@babel+core@7._25916515b1af5ea37c9cd7bb89130905/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../../../../next@14.2.33_@babel+core@7._25916515b1af5ea37c9cd7bb89130905/node_modules/next/dist/bin/next" "$@"
fi
