#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/@tinacms+cli@1.11.0_@codemi_dccfe8439c4f1dae4b4914c2e1d838d6/node_modules/@tinacms/cli/bin/node_modules:/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/@tinacms+cli@1.11.0_@codemi_dccfe8439c4f1dae4b4914c2e1d838d6/node_modules/@tinacms/cli/node_modules:/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/@tinacms+cli@1.11.0_@codemi_dccfe8439c4f1dae4b4914c2e1d838d6/node_modules/@tinacms/node_modules:/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/@tinacms+cli@1.11.0_@codemi_dccfe8439c4f1dae4b4914c2e1d838d6/node_modules:/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/@tinacms+cli@1.11.0_@codemi_dccfe8439c4f1dae4b4914c2e1d838d6/node_modules/@tinacms/cli/bin/node_modules:/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/@tinacms+cli@1.11.0_@codemi_dccfe8439c4f1dae4b4914c2e1d838d6/node_modules/@tinacms/cli/node_modules:/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/@tinacms+cli@1.11.0_@codemi_dccfe8439c4f1dae4b4914c2e1d838d6/node_modules/@tinacms/node_modules:/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/@tinacms+cli@1.11.0_@codemi_dccfe8439c4f1dae4b4914c2e1d838d6/node_modules:/mnt/e/_startup/moklabs/moklabs-landing/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@tinacms/cli/bin/tinacms" "$@"
else
  exec node  "$basedir/../@tinacms/cli/bin/tinacms" "$@"
fi
